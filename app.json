{
  "expo": {
    "name": "bankim",
    "slug": "bankim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5F9C3A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5F9C3A"
      },
      "package": "br.com.bankim"
    },
    "plugins": [
      "expo-sqlite",
      "expo-font"
    ]
  }
}
